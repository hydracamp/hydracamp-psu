<h1>Edit Zombie</h1>

<%= form_for @zombie do |f| %>
  <%=f.label :name %>
  <%=f.text_field :name %>
  <br>
  <%=f.label :graveyard %>
  <%=f.text_field :graveyard %>
  <%=f.submit %>
  <br>
  <%=f.label :nickname %>
  <%=f.text_field :nickname %>
  <%=f.submit %>
<% end %>

<%= link_to "home", zombies_path %>
